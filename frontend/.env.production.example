# Frontend Production Environment
# Copy to .env.production for production builds

# API Configuration (CHANGE THESE!)
API_URL=https://api.yourdomain.com
API_VERSION=v1
API_BASE_URL=${API_URL}/api/${API_VERSION}

# WebSocket
WS_URL=wss://api.yourdomain.com

# App Configuration
APP_NAME=Japanese Learning
APP_VERSION=1.0.0

# Environment
NODE_ENV=production

# Feature flags
ENABLE_DEBUG_LOGS=false
ENABLE_OFFLINE_MODE=true
ENABLE_ANALYTICS=true

# External services
GOOGLE_ANALYTICS_ID=your-production-ga-id
SENTRY_DSN=your-production-sentry-dsn